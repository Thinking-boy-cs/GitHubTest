<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.swu.pigeon.dao.ActivityDao">
    <insert id="submitAct" parameterType="Activity" useGeneratedKeys="true" keyProperty="id">
        insert into activity values (#{id},#{activityName},#{applicant},#{approver},#{startTime},#{endTime},#{status},#{submitTime})
    </insert>
</mapper>