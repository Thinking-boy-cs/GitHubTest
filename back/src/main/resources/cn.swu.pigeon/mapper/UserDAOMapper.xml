<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">



<mapper namespace="cn.swu.pigeon.dao.UserDAO">
    <insert id="save" parameterType="User" >
        insert into staff values (#{id},#{username},#{dept},#{password},#{sex},#{status},#{registerTime},#{icon},#{telNumber},#{email})
    </insert>
    <select id="findById" parameterType="String" resultType="User">
        select id,username,dept,password,sex,status,registerTime,icon
        from staff where id=#{id}
    </select>

</mapper>