<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.swu.pigeon.dao.GovernDao">
    <insert id="record" parameterType="Record">
        insert into signrecord values (#{id},#{temperature},#{nation},#{province},#{city},#{signTime})
    </insert>
    <select id="find" parameterType="String" resultType="User">
        select id,username,dept,password,sex,status,registerTime,icon,telNumber,email
        from staff
    </select>
</mapper>